{
  "name": "ocelot",
  "version": "1.0.0",
  "type": "module",
  "description": "museum back end",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "node --env-file=.env.local --watch src/server.ts",
    "start": "node --experimental-strip-types src/server.ts",
    "test": "node --test src/**/*.test.ts",
    "test:watch": "node --test --watch src/**/*.test.ts",
    "docs": "node --experimental-strip-types src/scripts/generate-doc.ts",
    "migrate": "node --experimental-strip-types scripts/migrate.ts up -j ts -m migrations",
    "migrate:up": "node --experimental-strip-types scripts/migrate.ts up -j ts -m migrations",
    "migrate:down": "node --experimental-strip-types scripts/migrate.ts down -j ts -m migrations",
    "migrate:create": "node --experimental-strip-types scripts/migrate.ts create -j ts -m migrations",
    "migrate:prod": "node --experimental-strip-types scripts/migrate.ts --prod up -j ts -m migrations",
    "migrate:prod:up": "node --experimental-strip-types scripts/migrate.ts --prod up -j ts -m migrations",
    "migrate:prod:down": "node --experimental-strip-types scripts/migrate.ts --prod down -j ts -m migrations"
  },
  "author": "kazerlelutin",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/postgres": "^6.0.2",
    "@fastify/schedule": "^6.0.0",
    "@fastify/websocket": "^11.2.0",
    "fastify": "^5.6.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "stripe": "^20.0.0",
    "toad-scheduler": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/pg": "^8.15.6",
    "knip": "^5.77.1",
    "node-pg-migrate": "^8.0.4",
    "pino-pretty": "^13.1.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
